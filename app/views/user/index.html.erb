<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

<h1><p class="text-success">PhotoSite User List</p></h1>
<body>

<p class="text-secondary">There are <%=User.count %> users</p>



<table class="table table-hover">
  <thead>
  <tr>
    <th scope="col">User</th>
    <th scope="col">First</th>
    <th scope="col">Last</th>
  </tr>
  </thead>
  <tbody>
  <% for user in @users_all %>
    <tr>
      <td>
        <%= user.id %>
      </td>

      <td>
        <%= link_to user.first_name, :controller => "photo", :id => user.id, :action => "index"  %>
      </td>

      <td>
        <%= link_to user.last_name, :controller => "photo", :id => user.id, :action => "index" %>
      </td>
    <tr/>
  <%end %>
  </tbody>
</table>

</body>
